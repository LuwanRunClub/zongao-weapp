.status{
  padding: 0 20rpx;
  height: 40rpx;
  line-height: 40rpx;
  border-radius: 30rpx;
  color: #fff;
  font-size: 22rpx;
}
.status.ing{
  background-color: #FF6D02;
}
.avatar{
  width: 74rpx;
  height: 74rpx;
  border: solid 2rpx #E1DDDD;
  border-radius: 50%;
}
.box{
  border-radius: 10rpx;  
  background: #fff;
  padding: 30rpx;
}
.box+.box{
  margin-top: 20rpx;
}
.box .hd{
  font-weight: bold;
}
.section{
  background-color: #fff;
  padding: 30rpx;
  margin-bottom: 18rpx;
}
.section .hd{
  font-size: 30rpx;
  font-weight: bold;
  color: #333;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.section .hd.pb{
  padding-bottom: 30rpx;
}
.hd navigator{
  color: #999;
  font-size: 26rpx;
  font-weight: normal;
}
.icon.mini{
  width: 35rpx;
  height: 35rpx;
}