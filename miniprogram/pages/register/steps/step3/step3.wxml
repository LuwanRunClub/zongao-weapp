<view class="box">
  <view class="title">报名信息：</view>
  <view class="cell">
    <label>比赛名称：</label>
    <text>{{order.raceTitle}}</text>
  </view>
  <view class="cell">
    <label>报名类别：</label>
    <text>{{order.cateTitle}}</text>
  </view>
  <view class="cell">
    <label>总费用：</label>
    <text>￥{{order.totalFee}}</text>
  </view>
  <view class="cell">
    <label>订单状态：</label>
    <text>待支付</text>
  </view>
</view>
<userlist profiles="{{order.profiles}}" />