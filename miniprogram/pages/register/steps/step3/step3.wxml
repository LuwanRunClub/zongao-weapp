<view class="box">
  <view class="title">报名信息：</view>
  <view class="cell">
    <label>比赛名称：</label>
    <text>{{order.raceTitle}}</text>
  </view>
  <view class="cell">
    <label>报名类别：</label>
    <text>{{order.cateTitle}}</text>
  </view>
  <view class="cell">
    <label>总费用：</label>
    <text>￥{{order.totalFee}}</text>
  </view>
  <view class="cell">
    <label>订单状态：</label>
    <text>待支付</text>
  </view>
</view>
<userlist profiles="{{order.profiles}}" />
<view class="box info">
  <view class="title">使用优惠券</view>
  <van-cell title="选择优惠券" value="{{coupon}}" border="{{ false }}" is-link />
  <van-cell-group>
    <van-field border="{{ false }}" value="{{ sms }}" center clearable label="优惠券代码" placeholder="请输入12位优惠券代码" border="{{ false }}"
      use-button-slot>
      <van-button slot="button" size="small" type="primary">添加</van-button>
    </van-field>
  </van-cell-group>
</view>