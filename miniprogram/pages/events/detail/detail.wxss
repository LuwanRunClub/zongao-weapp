.container{
  background-color: #f6f6f6;
  padding: 0;
}
swiper{
  height: 320rpx!important;
}
.swiper-item image{
  height: 320rpx!important;
  width: 690rpx;
  width: 100%;
  border-radius: 10rpx;
}
.cell{
  padding: 5rpx 0;
}
.cell text{
  letter-spacing: 2rpx;
  color: #666;
}
.cell .bold{
  font-weight: bold;
  color: #333;
}
.info{
  font-size: 26rpx;
  color: #666;
}
.header{
  padding: 20rpx 0;
}
.header .title{
  font-weight: bold;
}
