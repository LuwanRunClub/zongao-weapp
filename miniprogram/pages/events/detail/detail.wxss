.container{
  background-color: #f6f6f6;
  padding: 0;
  height: 100vh;
}
.section {
  padding: 30rpx;
}
.top{
  overflow: auto;
}
.bottom{
  padding: 20rpx 0;
  background-color: #fff;
  border-top: solid 2rpx #f5f5f5;
  padding-bottom: constant(safe-area-inset-bottom); /* 兼容 iOS < 11.2 */
  padding-bottom: env(safe-area-inset-bottom); /* 兼容 iOS >= 11.2 */
}
.bottom button{
  width: 300rpx;
  background-color: #fff;
}
map{
  width: 100%;
  border-radius: 20rpx;
  margin: 20rpx 0;
}
swiper{
  height: 320rpx!important;
}
.icons van-grid{
  --grid-item-icon-size: 48rpx;
}
.van-grid-item {
  margin-bottom: 20rpx;
}
.swiper-item image{
  height: 320rpx!important;
  width: 690rpx;
  width: 100%;
  border-radius: 10rpx;
}
.cell{
  padding: 5rpx 0;
}
.cell text{
  letter-spacing: 2rpx;
  color: #999;
}
.cell .text{
  color: #333;
}
navigator.link{
  display: inline-block;
  text-decoration: underline;
  color: cornflowerblue;
}
.info{
  font-size: 26rpx;
  color: #666;
}
.header{
  padding: 20rpx 0;
}
.header .title{
  font-weight: bold;
}
.empty{
  font-size: 24rpx;
  color: #999;
  padding-top: 20rpx;
}